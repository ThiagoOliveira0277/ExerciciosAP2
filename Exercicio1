public class Exercicio1 {
    public static void main(String[] args) {
        double c = 5.0;
        double v;
        v = Double.parseDouble(JOptionPane.showInputDialog("Digite o valor em dolares:"));
        v = v * c;
        JOptionPane.showMessageDialog(null, "O valor digitado em reais Ã©:"+v);
        
    }
    
}
