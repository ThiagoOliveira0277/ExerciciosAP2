import javax.swing.JOptionPane;
/**
 *
 * @author thiag
 */
public class Exercicio6 {
    public static void main (String args[]){
            int i;
            i = Integer.parseInt(JOptionPane.showInputDialog("Digite a sua idade:"));
            i = i * 365;
            JOptionPane.showMessageDialog(null, "Voce tem "+ i+ " dias de vida.");
    }
    
}
